{
  "private": true,
  "devDependencies": {
    "@parcel/optimizer-data-url": "^2.6.1",
    "@parcel/transformer-inline-string": "^2.6.1",
    "esbuild": "^0.14.45",
    "mocha": "^8.3.0",
    "mocha-headless-chrome": "^3.1.0",
    "parcel": "^2.6",
    "pulp": "^16.0.2",
    "purescript": "^0.15.2",
    "purescript-psa": "^0.8.2",
    "purs-tidy": "^0.9.0",
    "spago": "^0.20.9"
  },
  "scripts": {
    "fmt": "purs-tidy format-in-place src test",
    "build": "spago build",
    "build:watch": "spago build -w",
    "build:prod": "mkdir -p prod && cp -r static/** prod/ && rm -rf dist && spago bundle-app --to prod/index.js && parcel build prod/index.html",
